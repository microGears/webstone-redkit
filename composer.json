{
  "name": "microgears/webstone-redkit",
  "type": "library",
  "description": "Redis database access interface for PHP",
  "keywords": [
    "microGears",
    "webstone-redkit"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maxim Kirichenko",
      "email": "kirichenko.maxim@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-mbstring": "*",
    "ext-simplexml": "*",
    "microgears/webstone-stdlib": "^1.0"
  },
  "autoload": {
    "psr-4": {"WebStone\\Redkit\\": "src"}
  },
  "autoload-dev": {
    "psr-4": {"WebStone\\Redkit\\Test\\": "test"}
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  }
}
